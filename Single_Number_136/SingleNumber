package Single_Number_136;
public class SingleNumber {
    public int singleNumber(int[] nums) {
        int ans = 0;
        for (int n : nums) {
            ans ^= n;
        }
        return ans;
    }
    public static void main(String[] args) {
        SingleNumber obj = new SingleNumber();

        int[] nums = {2, 2, 1};
        System.out.println(obj.singleNumber(nums)); // 1
    }
}
